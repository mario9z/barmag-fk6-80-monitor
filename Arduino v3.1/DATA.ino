void triLeft (int x, int y)
{
  x += 8;
  VGA.fillTri(x, y, x + 10, y - 6, x + 10, y + 6, 1);
}

void triRight(int x, int y)
{
  x += 8;
  VGA.fillTri(x, y - 6, x, y + 6, x + 10, y, 1);
}
