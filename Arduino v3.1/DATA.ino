void clearMatrix()
{
  for (int y = 0; y < 48; y++)
  {
    for (int x = 0; x < 80; x++)
    {
      blinkData[y][x] = 0x00;
      cursorData[y][x] = 0x00;
    }
  }
}

void generateSpaceData(struct addr addressL)
{
  int y = addressL.yDec;
  for (int i = 2 ; i <= 77 ; i++)
  {
    VGA.moveCursor(i, y);
    VGA.write(0x20);
    blinkData[y][i] = 0x00;
    cursorData[y][i] = 0x00;
  }
}

